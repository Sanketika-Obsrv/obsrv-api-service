{
  "name": "obsrv-api-service",
  "version": "1.0.0",
  "description": "API service for the obsrv api's",
  "scripts": {
    "start": "ts-node ./src/app.ts",
    "test": "nyc mocha ./src/tests/**/*.spec.ts --exit",
    "actions:test": "nyc mocha ./src/tests/**/*.spec.ts --exit",
    "build": "tsc -P . --declaration",
    "build-package": "tsc -P . --declaration && node scripts/buildPackage.js",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "author": "Obsrv",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "dateformat": "2.0.0",
    "express": "^4.18.2",
    "http-status": "^1.5.3",
    "kafkajs": "^2.2.4",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multiparty": "4.2.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "prom-client": "^14.2.0",
    "sequelize": "^6.37.1",
    "uuid": "3.1.0",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "~3.2.1"
  },
  "overrides": {
    "semver": "^7.5.3",
    "@babel/traverse": "7.23.2"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@azure/storage-blob": "^12.23.0-alpha.20240402.1",
    "@google-cloud/storage": "^7.9.0",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-spies": "^1.0.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^2.0.1",
    "@types/kafkajs": "^1.9.0",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.190",
    "@types/mocha": "^10.0.0",
    "@types/mock-knex": "^0.4.4",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.6",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-spies": "^1.0.0",
    "diff-json": "^2.0.0",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "mocha": "^10.1.0",
    "nock": "^13.2.9",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "sinon": "^17.0.1"
  },
  "nyc": {
    "exclude": [
      "**/test/*.ts",
      "**/lib/**/*.js",
      "**/managers/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ]
